<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.opilane.punane">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/punane"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/punane"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.opilane.punane.LoginActivity" />
        <activity android:name="com.example.opilane.punane.PasswordActivity" />
        <activity android:name="com.example.opilane.punane.RegisterActivity" />
        <activity android:name="com.example.opilane.punane.UserProfileActivity" />
        <activity android:name="com.example.opilane.punane.LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>